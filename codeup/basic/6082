cnt = int(input())

for i in range(1, cnt+1):
    if i%10 != 0:
        if (i%10)%3 == 0:
            print('X', end=' ')
        else:
            print(i, end=' ')
    else:
        print(i)

---------------------------------------

cnt = int(input())

for i in range(1, cnt+1):
    if (i%10==3 or i%10==6 or i%10==9):
        print('X', end=' ')
        continue
    
    print(i, end=' ')
